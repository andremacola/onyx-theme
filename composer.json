{
	"name": "andremacola/onyx-theme",
	"description": "Onyx WP Theme",
	"homepage": "https://github.com/andremacola/onyx-theme",
	"type": "wordpress-theme",
	"authors": [
		{
			"name": "Andre Macola",
			"email": "andremacola@gmail.com"
		}
	],
	"scripts": {
		"onyx-dump": [
			"composer dump-autoload -o"
		]
	},
	"config": {
		"vendor-dir": "core/vendor"
	},
	"autoload": {
		"classmap": [
			"core/app",
			"core/includes",
			"core/api",
			"core/controllers"
		]
	},
	"require": {
		"timber/timber": "^1.16"
	}
}
